{
	"name": "mybulma-template",
	"version": "0.1.0",
	"description": "bulma with npm/yarn and node-sass",
	"main": "sass/mystyles.scss",
	"scripts": {
		"css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
		"css-watch": "npm run css-build -- --watch",
		"start": "npm run css-watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/killshot13/mybulma-template.git"
	},
	"keywords": [
		"bulma",
		"sass",
		"scss",
		"css",
		"styles",
		"template",
		"node-sass",
		"node.js"
	],
	"author": "killshot13",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/killshot13/mybulma-template/issues"
	},
	"homepage": "https://github.com/killshot13/mybulma-template#readme",
	"devDependencies": {
		"bulma": "^0.9.2",
		"node-sass": "^5.0.0"
	}
}
